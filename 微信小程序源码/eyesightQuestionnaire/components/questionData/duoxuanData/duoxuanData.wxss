/* components/questionData/duoxuanData/duoxuanData.wxss */
.questionWidth {
  margin: 20rpx 20rpx;
}


.questionContent {
  margin: 0 20rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #999;
}

.questionTitle{
  margin-left: 10rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.questionTitle .star {
  color: red;
}

.questionTitle .tips {
  font-weight: normal;
  font-size: 28rpx;
  color: #dd514c;
}

.duoxuanList {
  margin-left: 30rpx;
}

.duoxuanLabel {
  margin-top: 20rpx;
}

.duoxuanItem {
  display: inline;
  margin-left: 20rpx;
}

.wx-canvas {
  position: relative;
  height: 500rpx;
  width: 100%;
  margin-top: 10rpx;
}

.myChart{
  height: 500rpx;
  width: 100%;
}

.canvasImg {
  width: 100%;
  height: 500rpx;
}
